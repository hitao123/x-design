  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.0.0",
    "@x-design/theme": "workspace:*",
    "@x-design/utils": "workspace:*",
    "async-validator": "^4.2.5"
  }
}
