{
  "name": "@x-design/components",
  "version": "0.0.1",
  "description": "X Design Vue 3 Component Library",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
